    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.8",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.11.8",
    "@types/passport": "^1.0.4",
    "@types/passport-google-oauth": "^1.0.41",
    "@zerollup/ts-transform-paths": "^1.7.18",
